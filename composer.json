{
    "name":"kassko/object-hydrator",
    "authors": [
        {
            "name":"Kassko"
        }
    ],
    "license": "MIT",
    "description":"Object hydrator",
    "keywords": [
        "object", "hydrator", "orm", "model", "data", "source", "anticorruption", "adapter", "translator"
    ],
    "require": {
        "php":">=7.4.11",
        "doctrine/collections": "^1.6.2",
        "symfony/config": "^3.4|^4.0|^5.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.10.4",
        "fzaninotto/faker": "^1.9.1",
        "phpunit/phpunit":"^9.4",
        "symfony/dependency-injection": "^3.4|^4.0|^5.0",
        "symfony/expression-language": "^3.4|^4.0|^5.0",
        "symfony/yaml": "^3.4|^4.0|^5.0"
    },
    "suggest": {
        "doctrine/annotations": "Allows to keep compatibility with doctrine annotations for php 7 environment",
        "symfony/expression-language": "Allows to use evaluated expressions into annotations attributes"
    },
    "conflict": {
        "symfony/expression-language": "<3.4|>=6.0"
    },
    "autoload": {
        "psr-4": {
            "Kassko\\ObjectHydrator\\": "src",
            "Kassko\\FrameworkBridge\\Symfony\\ObjectHydrator\\": "framework-bridge/symfony/src"
        }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.4.11"
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
